{
    "name": "itliusha/quicksand-admin",
    "description": "Quickly build a beautiful backend management system.",
    "license": "MIT",
    "authors": [
        {
            "name": "itliusha",
            "email": "itliusha@qq.com"
        }
    ],
    "homepage": "https://github.com/itliusha/quicksandadmin",

    "keywords": ["Laravel", "QuicksandAdmin"],

    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "Itliusha\\QuicksandAdmin\\": "src/"
        },
        "files": [
            "src/Helper/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Itliusha\\QuicksandAdmin\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Itliusha\\QuicksandAdmin\\QuicksandAdminServiceProvider"
            ],
            "aliases": {
                "QuicksandAdmin": "Itliusha\\QuicksandAdmin\\Facades\\QuicksandAdmin"
            }
        }
    }
}